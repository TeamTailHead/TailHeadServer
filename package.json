{
  "name": "tailhead-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/TeamTailHead/TailHeadServer.git",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore --resolve-plugins-relative-to . --fix .",
    "lint:prettier": "prettier --ignore-path .gitignore --write .",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@tailhead/communicator": "^0.2.1",
    "lodash": "^4.17.21"
  }
}
